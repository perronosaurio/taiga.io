# Base URL for the application (used for webhook URLs)
NEXT_PUBLIC_BASE_URL=http://localhost:3000

# For production, change to your actual domain
# NEXT_PUBLIC_BASE_URL=https://your-domain.com

# For self hosted taiga, change this to your taiga instance
NEXT_PUBLIC_TAIGA_DEFAULT_URL=https://api.taiga.io

# Secret key for verifying incoming Taiga webhooks (set this in Taiga's webhook settings too)
TAIGA_WEBHOOK_SECRET=